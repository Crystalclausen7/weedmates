<div id="profileCont" class="">
    <div class="py-5 bg-black">
        <div class="container">
            <div class="row">

                <div class="col-xs">

                    <div id="userDemo" class="d-flex justify-content-left mb-1" style="height: 150px; width:150px">
                        <img class="card-img-top"
                            src="https://images.unsplash.com/photo-1544843784-26792fc045f7?ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60"
                            alt="Card image cap" class="">
                    </div>
                    <div>
                        <p id="username" class="text-light"> UserName</p>
                        <input value="Mary Jane" class="text-dark" {{#if Authenticated}} readonly {{/if}} id="usernameInput">
                    </div><br>
                    <div>
                        <p id="location" class="text-light"> Location</p>
                        <input value="add Location" class="text-dark" {{#if Authenticated}} readonly {{/if}} id="locationInput">
                    </div><br>
                </div>
                <div id="description" class="col-xs">
                    <textarea class="text-dark" {{#if Authenticated}} readonly
                        {{/if}}> Please add a brief Description of yourself here ... My name is Mary Jane lets smoke!</textarea>
                </div>
                <div id="weedPref" class="col-xs">
                    <input type="checkbox" id="weed1" name="strain1" value="Sativa">
                    <label class="text-light" for="strain1"> Sativa</label><br>
                    <input type="checkbox" id="weed2" name="strain2" value="Indica">
                    <label class="text-light" for="strain2"> Indica</label><br>
                    <input type="checkbox" id="weed3" name="strain3" value="Hybrid">
                    <label class="text-light" for="strain3"> Hybrid</label><br>
                    
                </div>
            </div>
            <div id="saveBtn" class=" row justify-content-center">
                <button class="btn-success justify-content-center">Save</button>
            </div>
        </div>
    </div>
</div>